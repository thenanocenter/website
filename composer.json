{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "anahkiasen/former": "^4.1",
        "binarycabin/brainblocks": "^1.2",
        "binarycabin/laravel-reporting": "^1.0",
        "binarycabin/laravel-uuid": "^1.0",
        "binarycabin/nano-units": "^1.0",
        "fideloper/proxy": "^4.0",
        "graham-campbell/markdown": "^10.0",
        "greggilbert/recaptcha": "^2.2",
        "kilroyweb/options": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.12"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/homestead": "^7.9",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
